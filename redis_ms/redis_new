Cgroup enabled: true, MEM: 60 GB, CPUS: 0-15
grp=redis, mem_gb=60, cpus_csv=0-15
sudo mkdir -p '/sys/fs/cgroup/lht21-group-redis'
echo 64424509440 | sudo tee '/sys/fs/cgroup/lht21-group-redis/memory.max'  >/dev/null
[[[Setting cpuset for redis]]]
echo '0-15' | sudo tee '/sys/fs/cgroup/lht21-group-redis/cpuset.cpus'  >/dev/null || true
SET CGROUP!!!
